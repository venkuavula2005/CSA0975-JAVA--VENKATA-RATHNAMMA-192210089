import java.util.*;
public class twentyone_easy {
    public static void main(String args[]){
        Scanner input = new Scanner(System.in);
        char a = input.nextLine().charAt(0);
        int n = input.nextInt();
        for(int i = 1; i <= n; i++){
            for(int j = 1; j <= i; j++){
                System.out.print(a);
            }
            System.out.println();
        }
    }

}
