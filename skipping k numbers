import java.util.Scanner;
public class Valid{
    public static void main(String[]args){
         Scanner sc = new Scanner(System.in);
         System.out.print("enter the number");
         Scanner input = new Scanner(System.in);
int m=input.nextInt();
int n=input.nextInt();
int k=input.nextInt();
for(int i=m;i<=n;i=i+k+1)
{
 System.out.print(i+" ");
}
}
}
