import java.util.Scanner;
public class vote{
    public static void main(String[]args){
        Scanner input=new Scanner(System.in);
        System.out.print("enter the age");
        int n=input.nextInt();
        if(n>=18){
            System.out.print("eligible for vote");
        }else{
            System.out.print("not eligible for vote");
        }
    }
}
