import java.util.Scanner;
public class Valid{
    public static void main(String[]args){
         Scanner sc = new Scanner(System.in);
         Scanner input=new Scanner(System.in);
         System.out.print("enter the number");
         int n=input.nextInt();
         int rev=0;
         while(n!=0)
         {
         int rem=n%10;
         rev=rev*10+rem;
         n=n/10;
        }
        System.out.print(rev);
    }
}
