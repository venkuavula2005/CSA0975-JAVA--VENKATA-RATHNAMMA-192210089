import java.util.Scanner;
public class factor{
    public static void main(String[]args){
        Scanner input=new Scanner(System.in);
            System.out.print("enter the number");
            int n=input.nextInt();
            System.out.print("the factors of "+n+"are ");
            for(int i=1;i<=n;i++){
                if(n%i==0){
                    System.out.print(i+" ");
                }
            }
        }
    }
